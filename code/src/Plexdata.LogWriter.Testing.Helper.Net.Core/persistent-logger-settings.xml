    <LogWriter>
      <Settings>
        <LogLevel>Trace</LogLevel>
        <LogTime>utc</LogTime>
        <LogType>json</LogType>
        <ShowTime>true</ShowTime>
        <PartSplit>|</PartSplit>
        <TimeFormat>s</TimeFormat>
