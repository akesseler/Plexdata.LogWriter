<?xml version="1.0" encoding="utf-8" ?>
<appSettings>
  <Plexdata>
    <LogWriter>
      <Settings>
        <LogLevel>Trace</LogLevel>
        <LogTime>utc</LogTime>
        <LogType>raw</LogType>
        <ShowTime>true</ShowTime>
        <PartSplit>|</PartSplit>
        <TimeFormat>s</TimeFormat>
        <FullName>false</FullName>
        <Culture>de-DE</Culture>
        <UseColors>true</UseColors>
        <QuickEdit>true</QuickEdit>
        <WindowTitle>Hello World (XML)</WindowTitle>
        <BufferSize>
          <Width>500</Width>
          <Lines>1000</Lines>
        </BufferSize>
        <Coloring>
          <Trace>
            <Background>Gray</Background>
            <Foreground>Black</Foreground>
          </Trace>
          <Debug>
            <Foreground>Gray</Foreground>
            <Background>Black</Background>
          </Debug>
          <Verbose>
            <Foreground>White</Foreground>
            <Background>Black</Background>
          </Verbose>
          <Message>
            <Foreground>White</Foreground>
            <Background>Black</Background>
          </Message>
          <Warning>
            <Foreground>Yellow</Foreground>
            <Background>Black</Background>
          </Warning>
          <Error>
            <Foreground>Red</Foreground>
            <Background>Black</Background>
          </Error>
          <Fatal>
            <Foreground>Gray</Foreground>
            <Background>DarkRed</Background>
          </Fatal>
          <Critical>
            <Foreground>Blue</Foreground>
            <Background>White</Background>
          </Critical>
          <Disaster>
            <Foreground>Red</Foreground>
            <Background>White</Background>
          </Disaster>
        </Coloring>
      </Settings>
    </LogWriter>
  </Plexdata>
</appSettings>
