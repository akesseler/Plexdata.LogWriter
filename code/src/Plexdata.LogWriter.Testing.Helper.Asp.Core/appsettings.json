{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Plexdata": {
    "LogWriter": {
      "Settings": {
        "LogLevel": "Trace",
        "LogTime": "utc",
        "LogType": "csv",
        "ShowTime": true,
        "PartSplit": "|",
        "TimeFormat": "s",
        "FullName": false,
        "Culture": "de-DE",
        "UseColors": true,
        "QuickEdit": true,
        "WindowTitle": "Hello World (JSON)",
        "BufferSize": {
          "Width": 250,
          "Lines": 500
        },
        "Coloring": {
          "Trace": {
            "Background": "Gray",
            "Foreground": "Black"
          },
          "Debug": {
            "Foreground": "Gray",
            "Background": "Black"
          },
          "Verbose": {
            "Foreground": "White",
            "Background": "Black"
          },
          "Message": {
            "Foreground": "White",
            "Background": "Black"
          },
          "Warning": {
            "Foreground": "Yellow",
            "Background": "Black"
          },
          "Error": {
            "Foreground": "Red",
            "Background": "Black"
          },
          "Fatal": {
            "Foreground": "Gray",
            "Background": "DarkRed"
          },
          "Critical": {
            "Foreground": "Black",
            "Background": "Red"
          },
          "Disaster": {
            "Foreground": "Black",
            "Background": "Red"
          }
        }
      }
    }
  }
}
